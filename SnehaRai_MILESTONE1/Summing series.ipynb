   "source": [
    "def summing(n):\n",
    " \n",
    "    S = 0\n",
    "    j=3\n",
    "    for i in range(1, n+1):\n",
    "        S += (i*j+1)/j\n",
    "        j=j+1\n",
    "     \n",
    "    return S\n",
    " \n",
    "# Driver Code\n",
    "n = int(input(\"Enter n:\"))\n",
    "print(\"The sum of n term is: \",summing(n))"
   ]
